{
	"name": "@mariozechner/tui",
	"version": "0.1.0",
	"description": "Terminal User Interface library with differential rendering for efficient text-based applications",
	"type": "module",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch --preserveWatchOutput",
		"clean": "rm -rf dist",
		"prepublishOnly": "npm run clean && npm run build",
		"check": "biome check . && tsc --noEmit"
	},
	"files": [
		"dist/**/*",
		"README.md"
	],
	"keywords": [
		"tui",
		"terminal",
		"ui",
		"text-editor",
		"differential-rendering",
		"typescript",
		"cli"
	],
	"author": "",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": ""
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.3",
		"@types/node": "^20.19.9",
		"typescript": "^5.0.0"
	},
	"types": "./dist/index.d.ts",
	"dependencies": {
		"@types/mime-types": "^2.1.4",
		"chalk": "^5.4.1",
		"marked": "^15.0.12",
		"mime-types": "^3.0.1"
	}
}